<div class="container">
<h1> <%= @survey.name %> </h1>
Public Link:<a id="survey_link" href="/surveys/"<%=@survey.id%>>/survey/<%=@survey.id%></a>
<ol>
<% @results.each_pair do |key, val| %>
  <li> <%= key %> </li>
  <ul>
    <% val.each do |choice| %>
      <li><% choice.each_pair do |k, v| %>
        <%= k %>: <%= v %></li>
        <% end %>
    <% end %>
  </ul>
<% end %>
</ol>
</div>
